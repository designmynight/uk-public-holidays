    "keywords": ["php","uk","public","holidays"],
    "homepage": "https://github.com/designmynight/laravel-elasticsearch",
    "require": {
        "php": ">=5.6",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "authors": [
        {
            "name": "DesignMyNight team",
            "homepage": "https://www.designmynight.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DesignMyNight\\UKPublicHolidays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesignMyNight\\UkPublicHolidays\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "license" : "MIT"
}
